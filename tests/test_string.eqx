alias: slen >> string.len 1

$xyz slen 3 =assert

"asdf" slen 4 =assert
"asdf jkle" slen 9 =assert
"asdf jkle " slen 10 =assert
" asdf jkle" slen 10 =assert
" asdf jkle " slen 11 =assert
"" slen 0 =assert
