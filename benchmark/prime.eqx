: sieve_of_eratosthenes ( limit -- tbl )
  var primes
  { } -> primes
  2 over ( limit ) to: i i -> primes[i] end
  2 over ( limit ) math.sqrt/1 to: i
    primes[i] if
      i i * over ( limit ) i step: j
        false -> primes[j]
      end
    then
  end
  [ ] ( result )
  2 rot ( limit  ) to: i
    primes i at if
      dup i append
    then
  end ;

os.clock/0
1000000 sieve_of_eratosthenes
os.clock/0 rot ( started ) - . cr

pairs: k v
  k . v . cr
end
